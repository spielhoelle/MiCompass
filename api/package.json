{
  "scripts": {
    "db-migrate": "cd ./src/db && tsc && cd ./../../db-build && sequelize db:migrate --url $DATABASE_URL && cd ../ && rimraf ./db-build",
    "db-seed": "cd ./src/db && tsc && cd ./../../db-build && sequelize db:seed:all --url $DATABASE_URL && cd ../ && rimraf ./db-build",
    "linter": "tslint '**/*.{ts,js,css}'",
    "prettier": "prettier --write '**/*.{ts,js,css,html,json}' && npx sort-package-json",
    "run-dev": "ts-node-dev --respawn --transpile-only ./src/server.ts",
    "run-prod": "NODE_ENV=production tsc && node ./build/src/server.js",
    "update-all": "npx npm-check-updates -u"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/faker": "^4.1.12",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.0.24",
    "@types/sequelize": "^4.28.9",
    "@types/supertest": "^2.0.10",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.19.0",
    "chalk": "4.1.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "2.8.5",
    "dotenv": "^8.2.0",
    "errorhandler": "1.5.1",
    "express": "^4.17.1",
    "express-status-monitor": "1.3.3",
    "jsonwebtoken": "8.5.1",
    "lusca": "1.6.1",
    "morgan": "^1.10.0",
    "nodemailer": "^6.4.10",
    "path": "0.12.7",
    "pg": "^8.3.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.3",
    "sequelize-cli": "^6.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.6",
    "faker": "^4.1.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "ts-node-dev": "^1.0.0-pre.52",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0"
  }
